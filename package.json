{
    "name": "fullstack-setup-2023",
    "version": "1.7.9",
    "description": "A personal fullstack app dev environment setup",
    "main": "src/index.ts",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "dev": "pnpm nodemon -L --development && vite",
        "provider": "ts-node src/seeds/index.ts && npm-run-all --parallel provider-server vite",
        "provider-server": "pnpm nodemon -L --provider",
        "vite-production": "vite preview --host 0.0.0.0 --port $PORT",
        "vite": "vite",
        "nodemon": "nodemon server.js",
        "consumer": "pnpm nodemon -L --consumer",
        "infrastructure": "pnpm nodemon -L --infrastructure",
        "start": "npm-run-all --parallel start-back vite-production",
        "start-back": "node dist/src/index.js",
        "staging": "node dist/index.js",
        "dev:env": "pnpx nodemon",
        "build": "npm-run-all --parallel build-backend build-front",
        "build-backend": "rimraf ./dist && tsc",
        "build-front": "vite build",
        "test": "ts-mocha -p tsconfig.json tests/**/*.spec.ts --timeout 4000",
        "prettier": "prettier --write ./src/",
        "lint": "eslint src/**/*.{ts,js}",
        "lint:fix": "eslint src/**/*.{ts,js} --fix",
        "postinstall": "git init && git config core.hooksPath ./.git-hooks && npm i -g semver",
        "seed": "ts-node src/seeds/index.ts",
        "setup:model": "node scripts/setupModel.mjs",
        "create:resource": "node scripts/createResource.mjs"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@mailchimp/mailchimp_transactional": "^1.0.50",
        "@types/bcrypt": "^5.0.2",
        "@types/chai": "^4.3.4",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.15",
        "@types/express-serve-static-core": "^4.17.41",
        "@types/express-session": "^1.17.10",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/mailchimp__mailchimp_transactional": "^1.0.9",
        "@types/mocha": "^10.0.1",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.11",
        "@types/node": "^18.11.17",
        "@types/nodemailer": "^6.4.14",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "chai": "^4.3.7",
        "eslint": "^8.30.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "inquirer": "^9.2.12",
        "mocha": "^10.2.0",
        "nodemon": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.1",
        "rimraf": "^3.0.2",
        "semver": "^7.5.4",
        "supertest": "^6.3.3",
        "ts-mocha": "^10.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@socket.io/admin-ui": "^0.5.1",
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "connect-mongo": "^5.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.4",
        "express-session": "^1.17.3",
        "express-validator": "^7.0.1",
        "google-auth-library": "^9.4.1",
        "googleapis": "^128.0.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "8.1.0",
        "morgan": "^1.10.0",
        "multer": "1.4.5-lts.1",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.9.7",
        "npm-run-all": "^4.1.5",
        "socket.io": "^4.6.2",
        "vite": "^5.4.6",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "resolutions": {
        "engine.io": "6.5.1"
    }
}
